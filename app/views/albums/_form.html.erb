<%= form_for @album, :url => albums_path, :html => { :multipart => true} do |f| %>
  <p>
    <%= f.label :user_id %><br />
    <%= f.collection_select :user_id, User.all, :id, :email, include_blank: false %>
  </p>
  <p>
    <%= f.file_field :photo %>
  </p>
  <p>
    <%= f.submit "Submit" %>
  </p>
  <% end %>
